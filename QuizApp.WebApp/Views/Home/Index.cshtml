@model IEnumerable<QuizApp.Dto.CategoryDtos.CategorySummary>
<h1>AAAAA</h1>
<div class="row">
    @foreach (var quiz in Model)
    {
        <div class="col-3 p-5 border border-3 bg-success rounded-3">
            <a asp-controller="Home" asp-action="QuizChoose" asp-route-id="@quiz.Id">@quiz.Name</a>
        </div>
    }
</div>
